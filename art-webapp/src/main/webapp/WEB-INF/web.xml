<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Art Gallery Software</display-name>

  <context-param>
    <param-name>AppName</param-name>
    <param-value>Art Gallery Web Application</param-value>
  </context-param>

  <servlet>
  <servlet-name>ArtGallery</servlet-name>
  <servlet-class>com.art.Hello</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>ArtGallery</servlet-name>
  <servlet-class>com.art.HelloGeneric</servlet-class>
  </servlet>

  <servlet-mapping>
  <servlet-name>ArtGalleryUser</servlet-name>
  <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>ArtGalleryUser</servlet-name>
  <url-pattern>/artsy</url-pattern>
  </servlet-mapping>


</web-app>
